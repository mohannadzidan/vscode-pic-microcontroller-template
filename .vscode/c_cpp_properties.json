{
    "env": {
        "xc8Path": "path-to-xc8-compiler",
        "chip": "chip-name"
    },
    "configurations": [
        {
            "name": "Windows",
            "includePath": [
                "${workspaceFolder}",
                "${xc8Path}/pic/include/*"
            ],
            "forcedInclude": [
                "${xc8Path}/pic/include/proc/${chip}.h"
            ],
            "defines": [],
            "intelliSenseMode": "clang-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${xc8Path}/include/*",
                    "${xc8Path}/pic/include/legacy/*"
                ],
                "databaseFilename": "",
                "limitSymbolsToIncludedHeaders": true
            },
            "cStandard": "c89",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}